{
  "name": "wordle-finder",
  "description": "A site allowing you to add generic wordlists for Wordle-like games and find the solution for a given date.",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "license": "AGPL-3.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build --outDir public",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "prettier \"**/*.{js,ts,svelte,json,md,yml}\"",
    "prepare": "yarn smui-theme-light && yarn smui-theme-dark",
    "smui-theme-light": "smui-theme compile smui/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile smui/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@smui/button": "^7.0.0-beta.0",
    "@smui/checkbox": "^7.0.0-beta.0",
    "@smui/common": "^7.0.0-beta.0",
    "@smui/data-table": "^7.0.0-beta.0",
    "@smui/dialog": "^7.0.0-beta.0",
    "@smui/icon-button": "^7.0.0-beta.0",
    "@smui/select": "^7.0.0-beta.0",
    "@smui/snackbar": "^7.0.0-beta.0",
    "@smui/textfield": "^7.0.0-beta.0",
    "@sveltejs/vite-plugin-svelte": "^2.0.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/seedrandom": "^3.0.2",
    "date-picker-svelte": "^2.2.5",
    "ls-proxy": "^0.7.0",
    "prettier": "^2.8.1",
    "prettier-plugin-svelte": "^2.9.0",
    "seedrandom": "^3.0.5",
    "smui-theme": "^7.0.0-beta.0",
    "svelte": "^3.54.0",
    "svelte-check": "^2.10.0",
    "svelte-material-ui": "^7.0.0-beta.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.0"
  }
}
